{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": [
    "plugin:react/recommended",
    "airbnb"
  ],
  "plugins": [
    "react"
  ],
  "rules": {
    "react/jsx-filename-extension": 0,
    "react/prop-types": 0,
    "arrow-parens": 0,
    "arrow-body-style": 0,
    "react/jsx-props-no-spreading": 0,
    "no-continue": 0
  }
}